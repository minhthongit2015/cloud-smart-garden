  <title><%= title %></title>
  <link rel="shortcut icon" type="image/png" href="/favicon-32x32.png" />
  <style>
    html,
    body,
    #root,
    .loading {
      width: 100%;
      height: 100%;
      margin: 0;
    }

    html,
    body {
      display: flex;
      justify-content: center;
      align-items: center;
    }
  </style>
</head>

<body>
  <div id="root"></div>
  <script src="/client-bundle.js"></script>
  
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-149485071-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-149485071-1');
  </script>
</body>

</html>