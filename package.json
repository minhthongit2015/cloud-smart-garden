  "scripts": {
    "tsc": "tsc",
    "tsc-dev": "ts-node-dev --respawn --transpileOnly src/server/server.ts",
    "tsc-prod": "tsc && node src/server/server.js",
    "watch": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "watchs": "webpack-dev-server --https --open --config webpack/webpack.dev.js",
    "watchfile": "webpack-dev-server --open --writeToDisk --config webpack/webpack.dev.js",
    "watchsfile": "webpack-dev-server --https --open --writeToDisk --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "node src/server/server.js",
    "install-tfjs-step-1": "echo https://github.com/nodejs/node-gyp#on-windows",
    "fix-tfjs-js": "npm install -g --production windows-build-tools | echo 'must run as admin'",
    "rebuild-tfjs": "npm rebuild @tensorflow/tfjs-node --build-from-source",
    "rebuild-sass": "npm rebuild node-sass",
    "install-global": "npm i -g eslint-import-resolver-webpack",
    "test": "node src/test/test.js",
    "build-audit": "webpack --config webpack/webpack.prod.audit.js",
    "check": "source-map-explorer 'public/*.js'",
    "audit": "npm run build-audit && npm run check",
    "bash": "heroku run bash -a beyond-garden-staing",
    "sizez": "du -a . | sort -n -r | head -n 20",
    "size": "npm run bash && npm run sizez"
  },
